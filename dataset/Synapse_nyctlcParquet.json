{
	"name": "Synapse_nyctlcParquet",
	"properties": {
		"linkedServiceName": {
			"referenceName": "global-ws-WorkspaceDefaultStorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"path": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"folderPath": {
					"value": "@concat('nyctlc/',dataset().path)",
					"type": "Expression"
				},
				"fileSystem": "landingzone"
			},
			"compressionCodec": "snappy"
		},
		"schema": [
			{
				"name": "AddressID",
				"type": "INT32"
			},
			{
				"name": "AddressLine1",
				"type": "UTF8"
			},
			{
				"name": "AddressLine2",
				"type": "UTF8"
			},
			{
				"name": "City",
				"type": "UTF8"
			},
			{
				"name": "StateProvince",
				"type": "UTF8"
			},
			{
				"name": "CountryRegion",
				"type": "UTF8"
			},
			{
				"name": "PostalCode",
				"type": "UTF8"
			},
			{
				"name": "rowguid",
				"type": "UTF8"
			},
			{
				"name": "ModifiedDate",
				"type": "INT96"
			}
		]
	}
}